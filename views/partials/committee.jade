section.team#committee
	div.container
			div.center.wow.fadeInDown
				h2 Committee
						blockquote.h3 “T- Together E- Everyone A- Achieves M- More”
							p
								small.lead Chambless 
						if committeemembers
							div.row
								each committeemember in committeemembers
									div.col-md-2.col-sm-4.col-xs-6.wow.fadeInDown.img-committee-container(prettyp)
										a(style="width:100%" href="#{committeemember.imageurl}" rel="prettyPhoto[pp_com]" )
											img.img-event.img-responsive.img-rounded.img-committee(src="#{committeemember.imageurl}"  alt="#{committeemember.name} #{committeemember.title}")
										p.lead.title #{committeemember.name}
										p.lead.title.center #{committeemember.title}
							
							